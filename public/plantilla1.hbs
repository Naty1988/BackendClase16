<table class="table">
   <thead>
      <tr>
         <th scope="col">Nombre</th>
         <th scope="col">Precio</th>
         <th scope="col">Imagen</th>
      </tr>
   </thead>
   <tbody>
      <tr>
         <td>
            {{this.nombre}}
         </td>
         <td>
            ${{this.precio}}
         </td>
         <td>
            <img style="height: 50px; width: 50px;" src="{{this.url}}">
         </td>
      </tr>
   </tbody>
</table>